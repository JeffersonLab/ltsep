import ltsep as lt

HCANAPATH=lt.SetPath.getPath("HCANAPATH")
REPLAYPATH=lt.SetPath.getPath("REPLAYPATH")
UTILPATH=lt.SetPath.getPath("UTILPATH")
PACKAGEPATH=lt.SetPath.getPath("PACKAGEPATH")
OUTPATH=lt.SetPath.getPath("OUTPATH")
ROOTPATH=lt.SetPath.getPath("ROOTPATH")
REPORTPATH=lt.SetPath.getPath("REPORTPATH")
CUTPATH=lt.SetPath.getPath("CUTPATH")
PARAMPATH=lt.SetPath.getPath("PARAMPATH")
SCRIPTPATH=lt.SetPath.getPath("SCRIPTPATH")
ANATYPE=lt.SetPath.getPath("ANATYPE")
USER=lt.SetPath.getPath("USER")
HOST=lt.SetPath.getPath("HOST")

BashPathEntry=("%s,%s,%s,%s,%s,%s,%s,%s,%s,%s,%s,%s,%s" % (HCANAPATH, REPLAYPATH, UTILPATH, PACKAGEPATH, OUTPATH, ROOTPATH, REPORTPATH, CUTPATH, PARAMPATH, SCRIPTPATH, ANATYPE, USER, HOST))
print(BashPathEntry)
